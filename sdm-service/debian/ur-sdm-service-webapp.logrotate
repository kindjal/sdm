
/var/log/ur-sdm-service/ur-sdm-service-webapp.log {
        weekly
        missingok
        rotate 10
        compress
        delaycompress
        notifempty
        create 644 www-data www-data
        sharedscripts
        postrotate
            /etc/init.d/ur-sdm-service-webapp restart > /dev/null 2>&1
        endscript
}

